{
	"name": "vscode-swift-langsrv",
	"description": "The language server for the vscode-swift extension.",
	"version": "0.0.1",
	"author": "David Owens II",
	"license": "MIT",
	"homepage": "https://github.com/owensd/vscode-swift/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/owensd/vscode-swift.git"
	},
	"engines": {
		"node": "*"
	},
	"dependencies": {
	},
	"devDependencies": {
		"vscode": "^0.11.14",
		"typescript": "^1.8.9"
	},
	"scripts": {
		"compile": "installServerIntoExtension ../extension ./package.json ./tsconfig.json && tsc -p .",
		"watch": "installServerIntoExtension ../extension ./package.json ./tsconfig.json && tsc --watch -p ."
	},
	"activationEvents": [
		"onLanguage:swift",
		"workspaceContains:Package.swift"
	]
}
